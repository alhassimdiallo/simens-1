function RAS(n)
{   n=n+77;  /* on incr�mente 1 a chaque fois qu'on ajoute un champ input // on n'incr�mente pas quand on ajoute en bas*/
	  document.AVIS.elements[n].value="";
	  document.AVIS.elements[n+1].value="";
